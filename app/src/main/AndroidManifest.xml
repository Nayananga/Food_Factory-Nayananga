<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.foodfactory">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".FindRecipies"
            android:label="Find Recipes"/>
        <activity
            android:name=".UpdateProduct"
            android:label="Update Product"/>
        <activity
            android:name=".Recipes"
            android:label="Recipes"/>
        <activity
            android:name=".Search"
            android:label="Search"/>
        <activity
            android:name=".EditProduct"
            android:label="Edit Product"/>
        <activity
            android:name=".Availability"
            android:label="Availability"/>
        <activity
            android:name=".DisplayProduct"
            android:label="Display Product"/>
        <activity
            android:name=".RegisterProduct"
            android:label="Register Product"/>
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.VIEW" />
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>


</manifest>

